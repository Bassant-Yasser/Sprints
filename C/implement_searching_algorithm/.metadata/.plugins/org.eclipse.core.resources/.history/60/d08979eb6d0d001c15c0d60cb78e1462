#include <stdio.h>
#include <stdlib.h>

typedef unsigned char   uint8_t;
typedef unsigned int   uint32_t;

int cmpfunc (const void * a, const void * b) {
   return ( *(int*)a - *(int*)b );
}

int main(void) {
	int arr[5] = {2, 5, 15, 1, 20};
	qsort(arr, 5, sizeof(int), cmpfunc);
}

