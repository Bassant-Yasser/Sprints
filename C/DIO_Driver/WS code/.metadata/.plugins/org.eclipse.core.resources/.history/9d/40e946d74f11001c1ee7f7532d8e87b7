#include "DIO.h"

void DIO_Init (const DIO_ConfigType *Config_Ptr)
{
	switch(Config_Ptr->port)
	{
	case PORT_A:
		PORTA_DIR = (PORTA_DIR & (1<<Config_Ptr->pin_number)) | (dir<<Config_Ptr->pin_number);
		break;
	}
}

